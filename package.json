{
  "name": "assiut-robotics-server",
  "version": "2.0.0",
  "description": "A robust Node.js/Express server for managing the Assiut Robotics Team platform",
  "main": "index.js",
  "homepage": "https://github.com/AssiutRoboticsWeb/AssiutRoboticsServer",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "validate-env": "node scripts/validate-env.js",
    "deploy": "node scripts/deploy.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "prepare": "husky install",
    "health": "curl http://localhost:3000/health",
    "health-light": "curl http://localhost:3000/health/light"
  },
  "keywords": [
    "nodejs",
    "express",
    "mongodb",
    "robotics",
    "team-management",
    "api",
    "production-ready",
    "secure",
    "scalable"
  ],
  "author": "Assiut Robotics Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.8.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cloudinary": "^2.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.2.1",
    "googleapis": "^144.0.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongoose": "^8.1.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.13",
    "nodemon": "^3.0.3",
    "speakeasy": "^2.0.0",
    "validator": "^13.12.0",
    "winston": "^3.17.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mohamed12345abdullah/Assiut_Robotics_Website.git"
  },
  "bugs": {
    "url": "https://github.com/mohamed12345abdullah/Assiut_Robotics_Website/issues"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
